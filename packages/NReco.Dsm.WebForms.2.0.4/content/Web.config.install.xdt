<configuration xmlns:xdt="http://schemas.microsoft.com/XML-Document-Transform">
	
	<system.web xdt:Transform="InsertIfMissing">
	</system.web>

	<system.web>
		<pages xdt:Transform="InsertIfMissing" clientIDMode="AutoID"/>
	</system.web>	
	
	<system.web>
		<pages>
			<controls xdt:Transform="InsertIfMissing"/>
		</pages>
	</system.web>
	
	<system.web>
		<pages>
			<namespaces xdt:Transform="InsertIfMissing"/>
		</pages>
	</system.web>
	
	<system.web>
		<compilation xdt:Transform="InsertIfMissing"/>
	</system.web>	
	
	<system.web>
		<compilation>
			<expressionBuilders xdt:Transform="InsertIfMissing"/>
		</compilation>
	</system.web>	
	
	<system.web>
		<compilation>
			<expressionBuilders>
				<add expressionPrefix="component" type="NReco.Dsm.WebForms.ComponentExpressionBuilder,NReco.Dsm.WebForms" xdt:Transform="InsertIfMissing" xdt:Locator="Match(expressionPrefix)"/>
				<add expressionPrefix="label" type="NReco.Dsm.WebForms.LabelExpressionBuilder,NReco.Dsm.WebForms" xdt:Transform="InsertIfMissing" xdt:Locator="Match(expressionPrefix)"/>				
			</expressionBuilders>
		</compilation>
	</system.web>	
	
	<system.web>
		<pages>
			<controls>
				<add tagPrefix="NIData" namespace="NI.Data.Web" assembly="NI.Data" 
					xdt:Transform="InsertIfMissing" xdt:Locator="Match(tagPrefix)"/>
				<add tagPrefix="NRecoWebForms" namespace="NReco.Dsm.WebForms" assembly="NReco.Dsm.WebForms" 
					xdt:Transform="InsertIfMissing" xdt:Locator="Match(tagPrefix)"/>					
			</controls>
		</pages>
	</system.web>
	
	<system.web>
		<pages>
			<namespaces>
				<add namespace="NReco.Application.Web" xdt:Transform="InsertIfMissing" xdt:Locator="Match(namespace)"/>
				<add namespace="NReco.Dsm.WebForms" xdt:Transform="InsertIfMissing" xdt:Locator="Match(namespace)"/>
			</namespaces>
		</pages>
	</system.web>		

</configuration>